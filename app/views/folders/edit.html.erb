<h1>Folders#edit</h1>
<p>Find me in app/views/folders/edit.html.erb</p>

<%= form_for @folder do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label :private %>
  <%= f.check_box :private %>

  <%= f.submit %>

<% end %>
